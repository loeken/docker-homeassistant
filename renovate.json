{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["homeassistant/home-assistant"],
      "groupName": "home-assistant",
      "automerge": true
    }
  ],
  "pinDigests": true
}
